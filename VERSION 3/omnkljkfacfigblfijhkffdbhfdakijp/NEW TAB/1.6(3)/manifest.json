{
    "manifest_version": 3,
    "name": "New Tabs",
    "version": "1.6",
    "description": "A custom new tab extension.",
    "permissions": ["storage", "tabs"],
    "chrome_url_overrides": {
        "newtab": "New Tab Page.html"
    },
    "action": {
        "default_popup": "NEW TAB.html",
        "default_icon": {
            "32": "icon-32.png",
            "128": "icon-128.png"
        }
    },
    "icons": {
        "32": "icon-32.png",
        "128": "icon-128.png"
    }
}
